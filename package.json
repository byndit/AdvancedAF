{
  "name": "@byndit/advancedaf",
  "version": "1.0.0",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "npx tslint -c tslint.json --project .",
    "prettier": "npx prettier --config .prettierrc --write src/**/*.{ts,scss,html,sass}",
    "prettier:staged": "npx pretty-quick --staged",
    "fix": "npm run lint -- --fix && npm run prettier",
    "fix:staged": "npm run prettier:staged"
  },
  "files": [
    "lib"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/src/.*\\.spec\\.ts$"
  },
  "keywords": [
    "AdvancedAF",
    "Azure",
    "Functions",
    "Azure Functions"
  ],
  "author": "BEYONDIT GmbH",
  "license": "mit",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bynd365/AdvancedAF"
  },
  "homepage": "https://www.beyond365.de/",
  "description": "Advanced AF",
  "devDependencies": {
    "@types/jest": "^21.1.10",
    "@types/node": "^14.14.22",
    "husky": "^3.1.0",
    "jest": "^21.2.1",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.2",
    "ts-jest": "^21.2.4",
    "tslint": "~5.15.0",
    "tslint-etc": "^1.13.9",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@azure/functions": "^1.2.3"
  }
}
